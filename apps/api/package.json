{
  "name": "@airy/api-dashboard",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "tsx watch src/server.ts",
    "build": "tsup",
    "start": "NODE_ENV=production node -r ./register-aliases.cjs dist/server.js",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "lint:ci": "eslint 'src/**/*.{ts,tsx}' --max-warnings 0",
    "format:write": "prettier --write 'src/**/*.{ts,tsx,js,json,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,json,md}'",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "node",
    "typescript",
    "express",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@prisma/client": "^6.16.2",
    "argon2": "^0.44.0",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pino": "^9.11.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "concurrently": "^8.2.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "module-alias": "^2.2.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2",
    "prisma": "^6.16.2",
    "supertest": "^7.1.4",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.5.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
